<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iwinner.hb.dto.Product" table="Product_Ger">
		<id name="productId" type="integer" column="productId">
		<generator class="com.iwinner.hb.utils.CustomGenerator"></generator>
		</id>
		<property name="proName" column="proName" />
		<property name="price" column="price" />
	</class>
</hibernate-mapping>